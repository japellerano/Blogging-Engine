<% @users.each do |user| %>
   <p class="left"><%= user.email %></p>
   <p class="left"><%= user.username %></p>
   <p class="right"><%= link_to 'Show', user %> | <%= link_to 'Edit', edit_user_path(user) %> | <%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></p>
<% end %>

